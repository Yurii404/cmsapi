<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
  <changeSet id="createTableExample" author="your_name">
    <createTable tableName="example_table">
      <column name="id" type="INT">
        <constraints primaryKey="true" nullable="false"/>
      </column>
      <column name="name" type="VARCHAR(255)"/>
    </createTable>
  </changeSet>

  <changeSet id="insertDataExample" author="your_name" dbms="mysql">
    <insert tableName="example_table">
      <column name="id" valueNumeric="1"/>
      <column name="name" value="Row 1"/>
    </insert>
    <insert tableName="example_table">
      <column name="id" valueNumeric="2"/>
      <column name="name" value="Row 2"/>
    </insert>
  </changeSet>
  <changeSet id="insertDataExamples" author="your_name" dbms="mysql">
    <insert tableName="example_table">
      <column name="id" valueNumeric="3"/>
      <column name="name" value="Row 3"/>
    </insert>
    <insert tableName="example_table">
      <column name="id" valueNumeric="4"/>
      <column name="name" value="Row 4"/>
    </insert>
  </changeSet>


</databaseChangeLog>
